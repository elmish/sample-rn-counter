{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "0.1.0",
    "command": "dotnet",
    "isShellCommand": true,
    "showOutput": "always",
    "suppressTaskName": true,
    "tasks": [
        {
            "taskName": "watch",
            "options": {
                "cwd": "${workspaceRoot}/src"
            },
            "args": [ "fable","yarn-watch" ],
            "isBackground": true,
            "problemMatcher": "$msCompile"
        }
    ]
}